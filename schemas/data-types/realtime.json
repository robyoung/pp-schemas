{
  "$schema": "http://json-schema.org/schema#",
  "title": "Realtime Google Analytics Data",
  "type": "object",
  "properties": {
    "for_url": {
      "description": "The 'filters' parameter from the Google Analytics query",
      "type": "string"
    },
    "unique_visitors": {
      "type": "integer",
      "minimum": 0
    }
  },
  "required": ["for_url", "unique_visitors"]
}
